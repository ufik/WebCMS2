{
    "name": "webcms2/webcms2",
    "type": "library",
    "description": "WebCMS2 content management system for Nette framework. This package contains system libraries and admin module.",
    "keywords": ["cms","nette-based", "doctrine2"],
    "homepage": "http://www.webcook.cz/WebCMS2",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Tomáš Voslař",
            "email": "tomas.voslar@webcook.cz",
            "homepage": "http://www.webcook.cz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
		"o5/grido": "1.*",
		"webcook/doctrine": "1.*@dev",
		"nella/console": "v0.8.0",
		"nella/gedmo": "v0.8.0",
		"webcook/bootstrap-form-renderer": "~2.0.0",
		"monolog/monolog" : "1.6.0",
		"knplabs/packagist-api": "1.*@dev",
		"ddtraceweb/monolog-parser": "@dev",
		"webcook/translator" : "0.*@dev",
		"phpunit/phpunit": "4.5.*@dev",
		"mockery/mockery": "0.*",
		"facebook/php-sdk-v4": "@dev"
    },
    "autoload": {
        "classmap": ["Common", "AdminModule/presenters/", "FrontendModule/presenters/", "libs", "AdminModule/model", "Entity", "tests"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.7.x-dev"
        }
    }
}
